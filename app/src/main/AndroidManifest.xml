<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iut.erwan.datacentersupervision" >
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SetPreferencesFragmentActivity"
            android:label="@string/menu_preferences" >
        </activity>
        <activity
            android:name=".StatsTEMPActivity"
            android:label="@string/label_statsTempActivity"
            android:configChanges="orientation|screenSize" >
        </activity>
        <activity
            android:name=".StatsMPActivity"
            android:label="@string/label_statsMPActivity"
            android:configChanges="orientation|screenSize">
        </activity>
        <activity
            android:name=".StatsDDActivity"
            android:label="@string/label_statsDDActivity"
            android:configChanges="orientation|screenSize">
        </activity>
        <activity
            android:name=".PlotTEMPActivity"
            android:label="@string/label_statsDDActivity"
            >
        </activity>
    </application>

</manifest>
